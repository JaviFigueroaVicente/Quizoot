{
    "version": 2,
    "builds": [
        {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
            "outputDirectory": "public"
        }
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ]
}